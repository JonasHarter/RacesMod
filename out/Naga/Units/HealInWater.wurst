package HealInWater
	import Cuff
	import Naga
	import WaterLocation

	init
		HealInWater x = new HealInWater()
		x.addBuff('B01J')
		x.addAbility('A02A')

	class HealInWater extends Cuff
		
		construct()
			super(true, true, false)
			
		override function condition(unit target) returns integer
			for ut in getGlobal_NagaUnits()
				if target.getTypeId() == ut and target.getPos().isWaterAll()
					return 1
			return 0